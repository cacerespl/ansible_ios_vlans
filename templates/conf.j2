{% for name in users %}
username {{ name }} privilege 15 secret the_password
{%endfor %}

{% for key,values in vlans|dictsort %}
vlan {{ values.id }}
 name {{ key }}
{%endfor%}

interface range g0/1-24
 switchport mode access
 switchport voice vlan {{ vlans.Voz.id }}
 switchport access vlan {{ vlans.Datos.id }}
 spanning-tree portfast
